<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_srv_sl_no"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="0dp"
            android:gravity="center_horizontal"
            android:padding="10dp"
            android:text="SL"
            android:textSize="@dimen/abc_text_size_menu_material" />


        <TextView
            android:id="@+id/tv_srv_date"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/tv_srv_sl_no"
            android:padding="10dp"

            android:text="Date"
            android:textSize="@dimen/abc_text_size_menu_material" />

        <TextView
            android:id="@+id/tv_srv_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_toRightOf="@+id/tv_srv_date"
            android:layout_toEndOf="@+id/tv_srv_date"
            android:layout_toStartOf="@+id/delete_srv_member_sl"
            android:layout_toLeftOf="@+id/delete_srv_member_sl"

            android:gravity="center_horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"

            android:text="Status"
            android:textSize="@dimen/abc_text_size_menu_material" />

        <ImageButton

            android:id="@+id/delete_srv_member_sl"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_horizontal"
            android:src="@drawable/ic_action_cancel" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight=".2">


        </LinearLayout>


        <!--   inner comments no need this component
